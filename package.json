{
  "name": "redis-js-starter",
  "version": "0.1.0",
  "main": "server/index.js",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "css": "bunx @tailwindcss/cli -i ./styles/styles.css -o ./public/styles.css",
    "dev": "bun css && bun run --watch .",
    "docker": "docker compose down && docker compose up -d --build",
    "docker:redis": "docker compose down && docker compose up -d redis",
    "format": "prettier . --write",
    "start": "bun .",
    "ts": "bunx tsc"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/openai": "^1.3.22",
    "ai": "^4.3.16",
    "connect-redis": "^9.0.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-handlebars": "^8.0.3",
    "express-session": "^1.18.1",
    "markdown-it": "^14.1.0",
    "redis": "^5.5.6",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "ws": "^8.18.2",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/express-session": "^1.18.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "tailwindcss": "^4.1.10"
  }
}
